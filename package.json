    "@architect/utils": "^1.2.3",
    "chalk": "^2.4.2",
    "cpr": "^3.0.1",
    "glob": "^7.1.4",
    "rimraf": "^2.6.3",
    "run-series": "^1.1.8"
  },
  "devDependencies": {
    "codecov": "^3.5.0",
